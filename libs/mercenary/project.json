{
  "root": "libs/mercenary",
  "sourceRoot": "libs/mercenary/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/mercenary",
        "main": "libs/mercenary/src/index.ts",
        "tsConfig": "libs/mercenary/tsconfig.lib.json",
        "assets": ["libs/mercenary/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/mercenary/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/mercenary"],
      "options": {
        "jestConfig": "libs/mercenary/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
